{
  "name": "cs405g_project",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "setup_db": "cd server && npm run setup_db",
    "seed_db": "cd server && npm run seed_db",
    "build": "concurrently \"npm run build_server\" \"npm run build_client\"",
    "build_server": "cd server && npm run build",
    "build_client": "cd client && npm run build",
    "postinstall": "concurrently \"cd client && npm install\" \"cd server && npm install\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kncarrier28/CS405G_Project.git"
